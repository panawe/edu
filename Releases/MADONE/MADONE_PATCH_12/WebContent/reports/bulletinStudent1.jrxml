	<queryString>
		<![CDATA[SELECT SC.SLOGAN, SC.DIRECTOR,SY.DESCRIPTION SCHOOLYEAR, TE.NAME TERMNAME, TE.TERM_ID, AVGS.CLASS_NAME CLASSNAME, AVGS.NBR_STUDENT NBR_OF_STUDENTS, SJ.NAME SUBJECTNAME,
CONCAT_WS(' ',TC.LAST_NAME,TC.MIDDLE_NAME, TC.FIRST_NAME) TEACHERNAME, ST.STUDENT_ID STUDENT_ID,  CONCAT_WS(' ', ST.LAST_NAME, ST.MIDDLE_NAME,ST.FIRST_NAME) STUDENTNAME, AVGS.GRADE_NAME,
AVGS.MAX_MARK,AVGS.CLASS_MARK CLASSMARK, AVGS.CLASS_RATIO CLASSRATIO,AVGS.EXAM_MARK EXAMMARK, AVGS.EXAM_RATIO EXAMRATIO, AVGS.AVERAGE_MARK  MOYENNE, BULL.RANK_NBR RANK,  AVGS.RANK_NBR, BULL.GRADE_NAME OVERALLGRADE

FROM  STUDENT ST,TERM TE,SUBJECT SJ, TEACHER TC,   SCHOOLYEAR SY, AVERAGES AVGS, SCHOOL SC, BULLETIN BULL

WHERE AVGS.SCHOOLYEAR_ID=SY.SCHOOLYEAR_ID
AND ST.STUDENT_ID = AVGS.STUDENT_ID
AND AVGS.TERM_ID = TE.TERM_ID
AND AVGS.SUBJECT_ID =SJ.SUBJECT_ID
AND AVGS.TEACHER_ID = TC.TEACHER_ID
AND AVGS.SCHOOL_ID = SC.SCHOOL_ID
AND AVGS.SCHOOLYEAR_ID= $P{yearId}
AND AVGS.TERM_ID=$P{termId}
AND ST.MATRICULE=$P{matricule}
AND AVGS.SCHOOL_ID=$P{schoolId}
AND BULL.STUDENT_ID = AVGS.STUDENT_ID
AND BULL.TERM_ID = AVGS.TERM_ID
AND BULL.SCHOOLYEAR_ID=AVGS.SCHOOLYEAR_ID

GROUP BY
SC.SLOGAN, SC.DIRECTOR,SY.DESCRIPTION , TE.NAME ,  AVGS.GRADE_NAME, TE.TERM_ID,AVGS.CLASS_NAME,AVGS.NBR_STUDENT ,SJ.NAME ,
CONCAT_WS(' ',TC.LAST_NAME,TC.MIDDLE_NAME, TC.FIRST_NAME) ,ST.STUDENT_ID ,CONCAT_WS(' ', ST.LAST_NAME, ST.MIDDLE_NAME,ST.FIRST_NAME) ,
